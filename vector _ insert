for ( auto i : original )
cout << " THis is the original array " << a[i]<< endl;

original.insert( original.begin() + 2, temp.begin() + 3 , temp.begin () + 6)

for ( auto i : original }
cout << " This is the original array after inserting arrays from temp "<< a[i];
cout << endl ;


} 


